{
    "version": 2,
    "name": "petGramEnmanuelCast",
    "builds": [
        {"use": "@now/static-build",
        "src": "package.json" }
    ],
    "routes": [
        {
            "src": "(.*).json", "dest": "/$1.json"

        },
        {
            "src": "(.*).js", "dest": "/$1.js"

        },
        {
            "src": "/.*", "dest": "/index.html"
        }
    ]
}